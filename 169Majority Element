class Solution {
    public int majorityElement(int[] nums) {
        Arrays.sort(nums);
        int count=0;
        int max=0;
        
      
            for(int j=0;j<nums.length-1;j++){
                if(nums[j]==nums[j+1]){
                    count++;
                    max=Math.max(count,max);
                }
                else
                    count=0;
            }
         return nums[max];
        }
       
        
    }
