class Solution {
    public void moveZeroes(int[] nums) { 
        int j=0;
        int[] Array=new int[nums.length];
        for(int i=0;i<nums.length;i++){
            if(nums[i]!=0){
                int t=nums[i];
                Array[j]=t;
                nums[i]=0;
                j++;
            }
}
        for(int i=0;i<nums.length;i++){
            nums[i]=Array[i];
        }
        
    }
}